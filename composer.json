    "autoload": {
        "classmap": [
            "src/exceptions.php"
        ],

        "psr-0": {
            "Kalnoy\\Cruddy\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "v0.5.x-dev"
        }
    }
}
